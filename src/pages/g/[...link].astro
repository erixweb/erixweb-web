---
const pages = [
    {
        link: "twitter",
        redirect: "https://twitter.com/erixweb"
    },
    {
        link: "github",
        redirect: "https://github.com/erixweb"
    },
    {
        link: "wetspain",
        redirect: "https://wetspain.vercel.app"
    },
    {
        link: "socketgame",
        redirect: "https://github.com/erixweb/socketgame",
    }
]
const { link } = Astro.params 
const page = pages.find((page) => page.link === link);
return Astro.redirect(page.redirect)
